        <div id="ListDiv" class="container">

            <form action="/FosterManageList" method="POST">
                <table id="MyListTable" style="border-collapse:separate; border-spacing:10px 10px;">
                    <tr class="MyListTr">
                        <td>寵物暱稱：</td>
                        <td colspan="3"><input type="text" id="ListText" class="inputNotOutline" name="PetName"
                                value="">

                        </td>
                    </tr>

                    <tr class="MyListTr">
                        <td>送審日期(起)：</td>
                        <td><input type="date" name="FosterDateStart" class="inputNotOutline"></td>
                    </tr>
                    <tr class="MyListTr">
                        <td>送審日期(迄)：</td>
                        <td><input type="date" name="FosterDateEnd" class="inputNotOutline"></td>
                    </tr>

                    <tr class="MyListTr">
                        <td>狀態：</td>
                        <td>
                            <input type="radio" name="AdoptState" value="1" checked>
                            <label for="html" class="MyListLabel">已批准</label>
                            <input type="radio" name="AdoptState" value="0">
                            <label for="html" class="MyListLabel">未批准
                            </label>
                        </td>
                    </tr>

                    <tr class="MyListTr">
                        <td colspan="4">
                            <input type="submit" class="ListSubmit" value="查詢">
                            <input type="submit" class="ListSubmit" value="清除">
                        </td>
                    </tr>
                </table>
            </form>
